<?xml version='1.0' encoding='utf-8'?>
<widget id="io.md.blaze" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Blaze</name>
    <description>
        A sample firebase application for developer.
    </description>
    <author email="danish1023@gmail.com" href="http://mddanish.info">
        Md Danish
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="GoogleService-Info.plist" />
    </platform>
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="cordova-plugin-x-toast" spec="~2.7.0" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.4.4" />
    <plugin name="cordova-plugin-apprate" spec="~1.4.0" />
    <plugin name="cordova-plugin-dialogs" spec="~2.0.2" />
    <plugin name="cordova-plugin-spinner" spec="~1.1.0" />
    <plugin name="cordova-plugin-device" spec="~2.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="phonegap-plugin-push" spec="~2.2.3">
        <variable name="FCM_VERSION" value="17.0.0" />
    </plugin>
    <engine name="ios" spec="^4.5.5" />
</widget>
